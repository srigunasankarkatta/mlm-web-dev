{
  "name": "quadra-core",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:development": "vite --mode development",
    "dev:staging": "vite --mode staging",
    "dev:production": "vite --mode production",
    "build": "vite build",
    "build:development": "tsc -b && vite build --mode development",
    "build:staging": "tsc -b && vite build --mode staging",
    "build:production": "tsc -b && vite build --mode production",
    "preview": "vite preview",
    "preview:development": "vite preview --mode development",
    "preview:staging": "vite preview --mode staging",
    "preview:production": "vite preview --mode production",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist",
    "start": "npm run dev:development"
  },
  "dependencies": {
    "@tanstack/react-query": "^5.85.6",
    "@tanstack/react-query-devtools": "^5.85.6",
    "@types/d3": "^7.4.3",
    "@types/react-paginate": "^7.1.4",
    "ag-grid-community": "^34.1.2",
    "ag-grid-react": "^34.1.2",
    "autoprefixer": "^10.4.21",
    "axios": "^1.11.0",
    "clsx": "^2.1.1",
    "formik": "^2.4.6",
    "framer-motion": "^12.23.12",
    "lucide-react": "^0.539.0",
    "postcss": "^8.5.6",
    "react": "^18.2.0",
    "react-d3-tree": "^3.6.6",
    "react-dom": "^18.2.0",
    "react-icons": "^5.5.0",
    "react-paginate": "^8.3.0",
    "react-router-dom": "^6.8.0",
    "recharts": "^3.1.2",
    "swiper": "^11.2.10",
    "tailwindcss": "^3.3.0",
    "yup": "^1.7.0"
  },
  "devDependencies": {
    "@eslint/js": "^8.0.0",
    "@types/node": "^20.0.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.0.0",
    "eslint": "^8.0.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.0",
    "globals": "^13.0.0",
    "sass": "^1.91.0",
    "typescript": "^5.0.0",
    "vite": "^4.4.0"
  }
}
